{
  "name": "ts-fast-react-hooks",
  "version": "0.0.0",
  "description": "Boilerplate for building React hook libraries in TypeScript",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "check": "tsc",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "verify": "npm run check && npm run test && npm run build",
    "prepublishOnly": "npm run verify"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.8",
    "jsdom": "^26.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "engines": {
    "node": "^20.0.0 || >=22.0.0"
  }
}
